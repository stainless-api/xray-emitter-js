{
  "name": "@stainlessdev/xray-emitter",
  "version": "0.1.0",
  "type": "module",
  "repository": {
    "type": "git",
    "url": "https://github.com/stainless-api/xray-emitter-js.git"
  },
  "packageManager": "pnpm@10.28.1+sha512.7d7dbbca9e99447b7c3bf7a73286afaaf6be99251eb9498baefa7d406892f67b879adb3a1d7e687fc4ccc1a388c7175fbaae567a26ab44d1067b54fcb0d6a316",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./internal": {
      "types": "./dist/internal.d.ts",
      "import": "./dist/internal.js",
      "require": "./dist/internal.cjs"
    },
    "./node": {
      "types": "./dist/node.d.ts",
      "import": "./dist/node.js",
      "require": "./dist/node.cjs"
    },
    "./fetch": {
      "types": "./dist/fetch.d.ts",
      "import": "./dist/fetch.js",
      "require": "./dist/fetch.cjs"
    },
    "./express": {
      "types": "./dist/express.d.ts",
      "import": "./dist/express.js",
      "require": "./dist/express.cjs"
    },
    "./hono": {
      "types": "./dist/hono.d.ts",
      "import": "./dist/hono.js",
      "require": "./dist/hono.cjs"
    },
    "./fastify": {
      "types": "./dist/fastify.d.ts",
      "import": "./dist/fastify.js",
      "require": "./dist/fastify.cjs"
    },
    "./next": {
      "types": "./dist/next.d.ts",
      "import": "./dist/next.js",
      "require": "./dist/next.cjs"
    },
    "./remix": {
      "types": "./dist/remix.d.ts",
      "import": "./dist/remix.js",
      "require": "./dist/remix.cjs"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "test": "tsx --test src/**/*.test.ts",
    "typecheck": "tsc --noEmit",
    "lint": "oxlint src",
    "format": "oxfmt --write examples src",
    "format:check": "oxfmt --check examples src",
    "clean": "rm -rf dist",
    "release": "node scripts/release.js",
    "example:edge": "pnpm --filter ./examples/edge start",
    "example:express": "pnpm --filter ./examples/express start",
    "example:fastify": "pnpm --filter ./examples/fastify start",
    "example:hono": "pnpm --filter ./examples/hono start",
    "example:next": "pnpm --filter ./examples/next-app start",
    "example:node-http": "pnpm --filter ./examples/node-http start",
    "example:remix": "pnpm --filter ./examples/remix-app start"
  },
  "dependencies": {
    "@opentelemetry/api": "^1.9.0",
    "@opentelemetry/core": "^2.2.0",
    "@opentelemetry/exporter-trace-otlp-proto": "^0.200.0",
    "@opentelemetry/resources": "^2.2.0",
    "@opentelemetry/sdk-trace-base": "^2.2.0",
    "@opentelemetry/semantic-conventions": "^1.29.0"
  },
  "devDependencies": {
    "@types/express": "^5.0.0",
    "@types/node": "^20.19.29",
    "esbuild": "^0.25.0",
    "express": "^5.2.1",
    "fastify": "^5.6.2",
    "hono": "^4.11.4",
    "oxfmt": "^0.24.0",
    "oxlint": "^1.46.0",
    "react-router": "^7.9.3",
    "tsup": "^8.0.0",
    "tsx": "^4.19.0",
    "typescript": "^5.9.3"
  }
}
